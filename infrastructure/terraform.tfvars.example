# Example Terraform Variables
# Copy this file to terraform.tfvars and fill in your values
# DO NOT commit terraform.tfvars to version control

# Required: TURN server shared secret
# Generate with: openssl rand -base64 32
turn_secret = "your-turn-secret-here"

# Optional: SSH public key for TURN server access
# ssh_public_key = "ssh-rsa AAAAB3NzaC1yc2E..."

# Optional: Custom CDN domain
# cdn_custom_domain = "cdn.syncwatch.example"

# Optional: CDN certificate ID
# cdn_certificate_id = "your-certificate-id"
